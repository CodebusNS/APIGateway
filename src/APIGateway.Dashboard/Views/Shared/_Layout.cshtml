<!DOCTYPE html>
<html lang="zh-cn">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>APIGateway - @ViewData["Title"] </title>
    <link href="~/css/site.css" rel="stylesheet" />
    @RenderSection("css", false)
</head>
<body>
    <div id="nav" class="nav">
        <p class="navTitle">API Gateway</p>
        <div id="items">
            <div class="item" path="home">
                <img src="~/imgs/dashboard.png" /><span>DASHBOARD</span>
            </div>
            <div class="item" path="discoveryprovider">
                <img src="~/imgs/service-provider.png" /><span>DISCOVERY PROVIDER</span>
            </div>
            <div class="titleItem">
                <p>API GATEWAY</p>
                <div class="item">
                    <img src="~/imgs/services.png" /><span>SERVICES</span>
                </div>
                <div class="item">
                    <img src="~/imgs/routes.png" /><span>ROUTES</span>
                </div>
                <div class="item">
                    <img src="~/imgs/aggregates.png" /><span>AGGREGATES</span>
                </div>
            </div>
        </div>
    </div>

    <div id="body" class="body">
        <div id="head" class="head">
            <div>

            </div>
        </div>

        <div id="content" class="content">
            @RenderBody()
        </div>

        <div id="bottom" class="bottom">
            <div style="color:rgb(39,187,156);margin-top:6px;font-weight:bold;font-size:14px;margin-left:20px;display:inline-block;">APIGateway 1.0.0</div>
            <div style="color:rgb(90,115,142);margin-top:6px;font-weight:bold;font-size:14px;margin-left:20px;display:inline-block;">
                <img src="~/imgs/github.png" style="width:16px;margin-right:5px;position:relative;top:1px;" />
                <span>GitHub</span>
            </div>
            <div style="color:rgb(90,115,142);margin-top:6px;font-weight:bold;font-size:14px;margin-left:20px;display:inline-block;">
                <img src="~/imgs/Issues.png" style="width:16px; position:relative;top:2px;margin-right:5px;" /><span>Issues</span>
            </div>
        </div>
    </div>


    <script src="~/lib/jquery/dist/jquery.js"></script>
    <script src="~/lib/jquery-cookie/jquery.cookie.js"></script>
    <script src="~/js/site.js"></script>
    @RenderSection("js", false)
</body>
</html>
